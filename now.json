{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/match",
      "dest": "/match.js"
    },
    {
      "src": "/like",
      "dest": "/like.js"
    },
    {
      "src": "/manipulate",
      "dest": "/manipulate.js"
    },
    {
      "src": "/pub",
      "dest": "/pub.js"
    },
    {
      "src": "/songx",
      "dest": "/songx.js"
    },
    {
      "src": "/UnblockMusic\\w*/getNeteaseSongInfo",
      "dest": "/dolby.js"
    },
    {
      "src": "/UnblockMusic\\w*/getKuwoSongInfo",
      "dest": "/dolby.js"
    },
    {
      "src": "/.*",
      "dest": "/null.js"
    }
  ]
}